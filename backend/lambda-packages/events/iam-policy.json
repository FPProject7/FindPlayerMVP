{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:GetItem",
        "dynamodb:PutItem",
        "dynamodb:UpdateItem",
        "dynamodb:DeleteItem",
        "dynamodb:Query",
        "dynamodb:Scan"
      ],
      "Resource": [
        "arn:aws:dynamodb:us-east-1:325298451465:table/findplayer-events",
        "arn:aws:dynamodb:us-east-1:325298451465:table/findplayer-events/index/*",
        "arn:aws:dynamodb:us-east-1:325298451465:table/findplayer-event-registrations",
        "arn:aws:dynamodb:us-east-1:325298451465:table/findplayer-event-registrations/index/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::findplayer-event-images-325298451465/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:us-east-1:325298451465:*"
    }
  ]
} 